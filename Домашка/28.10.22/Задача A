import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        long res = 1;
        for (int i = 1; i < 34; i++) {
            res*=i;
        }

        System.out.println(res);
    }

    /** Эта штука считает сумму двух чисел */
    static int getSum(int num1, int num2){
        return num1+num2;
    }

    static char compare(int num1, int num2){
        if(num1 > num2) return '>';
        if(num1 < num2) return '<';
        return '=';
    }

}
